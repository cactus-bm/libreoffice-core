URE
{
    Settings
    {
        downloadname URE_{productversion}_{os}_install_{languages}
        variables
        {
            PRODUCTNAME URE
            PRODUCTVERSION 1.2
            PACKAGEVERSION 1.2
            PACKAGEREVISION 1
            LICENSENAME LGPL
            NOVERSIONINDIRNAME 1
            DOWNLOADBANNER urebanner.bmp
            DOWNLOADBITMAP urebitmap.bmp
            DOWNLOADSETUPICO ooosetup.ico
            DONTUSESTARTMENUFOLDER 1

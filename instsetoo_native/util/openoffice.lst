            STARTCENTER_TEMPLREP_URL http://templates.libreoffice.org/
            STARTCENTER_LAYOUT_STYLE 0
            REGISTRATION_PRODUCT LibreOffice
            REGISTRATION_CID 926117
            DICT_REPO_URL http://extensions.libreoffice.org/dictionaries/
		}

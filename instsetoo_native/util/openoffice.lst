        include {localpath}/misc/ure,{solarenvpath}/{os}/gcc3.4.1_glibc2.2.4_bits,{solarpath}/bin.{minor}/ure,{solarpath}/bin.{minor},{solarpath}/lib.{minor},{solarenvpath}/{os}/MS

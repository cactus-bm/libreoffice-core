// Menus -----------------------------------------------------------------

Menu RID_FILEVIEW_CONTEXTMENU
{
    ItemList =
    {
        MenuItem
        {
            Identifier = MID_FILEVIEW_DELETE ;
            HelpId = HID_FILEVIEW_MENU_DELETE ;
            Text [ de ] = "~Löschen" ;
            Text [ en-US ] = "~Delete";
        };
        MenuItem
        {
            Identifier = MID_FILEVIEW_RENAME ;
            HelpId = HID_FILEVIEW_MENU_RENAME ;
            Text [ de ] = "~Umbenennen" ;
            Text [ en-US ] = "~Rename";
        };
    };
};

ModalDialog DLG_SVT_QUERYDELETE
{
    SVLook = TRUE ;
    OutputSize = TRUE ;
    Moveable = TRUE ;
    Size = MAP_APPFONT ( 221 , 67 ) ;
    Text [ de ] = "Löschbestätigung" ;
    Text [ en-US ] = "Confirm Delete" ;

    FixedText TXT_ENTRY
    {
        NoLabel = TRUE;
        Pos = MAP_APPFONT ( 6 , 6 ) ;
        Size = MAP_APPFONT ( 40 , 10 ) ;
        Text [ en-US ] = "Entry:" ;
        Text [ de ] = "Eintrag:" ;
    };

    FixedText TXT_ENTRYNAME
    {
        Pos = MAP_APPFONT ( 52 , 6 ) ;
        Size = MAP_APPFONT ( 163 , 10 ) ;
        NoLabel = TRUE ;
    };

    FixedText TXT_QUERYMSG
    {
        NoLabel = TRUE;
        WordBreak = TRUE;
        Pos = MAP_APPFONT ( 6 , 19 ) ;
        Size = MAP_APPFONT ( 209 , 22 ) ;
        Text [ de ] = "Sind Sie sicher, dass Sie die selektierten Daten löschen wollen?" ;
        Text [ en-US ] = "Are you sure you want to delete the selected data?" ;
    };

    PushButton BTN_YES
    {
        Pos = MAP_APPFONT ( 6 , 47 ) ;
        Size = MAP_APPFONT ( 50 , 14 ) ;
        TabStop = TRUE ;
        DefButton = TRUE ;
        Text [ de ] = "~Löschen" ;
        Text [ en-US ] = "~Delete" ;
    };

    PushButton BTN_ALL
    {
        Pos = MAP_APPFONT ( 59 , 47 ) ;
        Size = MAP_APPFONT ( 50 , 14 ) ;
        TabStop = TRUE ;
        Disable = TRUE ;
        Text [ de ] = "~Alle löschen" ;
        Text [ en-US ] = "Delete ~All" ;
    };

    PushButton BTN_NO
    {
        Pos = MAP_APPFONT ( 112 , 47 ) ;
        Size = MAP_APPFONT ( 50 , 14 ) ;
        TabStop = TRUE ;
        Text [ de ] = "~Nicht löschen" ;
        Text [ en-US ] = "Do ~Not Delete" ;
    };

    CancelButton BTN_CANCEL
    {
        Pos = MAP_APPFONT ( 165 , 47 ) ;
        Size = MAP_APPFONT ( 50 , 14 ) ;
        TabStop = TRUE ;
    };
};






















    String DFA_WEEKSINYEAR_DESC // Description
    {
        Text [ de ] = "Bestimmt die Anzahl an Wochen im Jahr zu einem Datum" ;
        Text [ en-US ] = "Returns the number of weeks in the year in relation to a date";
    };


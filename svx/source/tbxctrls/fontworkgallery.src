ModalDialog RID_SVX_MDLG_FONTWORK_GALLERY
{
    OutputSize = TRUE;
    SVLook = TRUE ;
    Moveable = TRUE;
    Closeable = TRUE;

    Size = MAP_APPFONT( WIDTH, HEIGHT );

    Text [ de ] = "Fontwork Gallery" ;
    Text [ en-US ] = "Fontwork Gallery" ;

    FixedLine FL_FAVORITES
    {
        Pos = MAP_APPFONT ( 3 , 3 ) ;
        Size = MAP_APPFONT ( WIDTH - 6 , 8 ) ;

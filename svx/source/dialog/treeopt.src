ModalDialog RID_OFADLG_OPTIONS_TREE
{
    HelpID = HID_OFADLG_OPTIONS_TREE;
    Moveable = TRUE ;
    OutputSize = TRUE ;
    SVLook = TRUE ;

    Size = MAP_APPFONT ( COL_12 , ROW_5 ) ;
    Text [ de ] = "Optionen";
    Text [ en-US ] = "Options";

    OKButton PB_OK
    {
        Pos = MAP_APPFONT ( COL_4, ROW_3 ) ;
        Size = MAP_APPFONT ( RSC_CD_PUSHBUTTON_WIDTH , RSC_CD_PUSHBUTTON_HEIGHT ) ;

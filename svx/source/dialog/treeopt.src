    FixedImage IMG_HELP
    {
        Pos = MAP_APPFONT ( COL_3_1, ROW_1_1 ) ;
        Size = MAP_APPFONT ( WIDTH_IMG_HELP, HEIGHT_IMG_HELP ) ;
        Hide = TRUE;
    };

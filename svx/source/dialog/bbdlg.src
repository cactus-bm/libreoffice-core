        {
            PageItem
            {
                Identifier = RID_SVXPAGE_BORDER ;
                Text [ en-US ] = "Borders" ;
            };
            PageItem
            {
                Identifier = RID_SVXPAGE_BACKGROUND ;
                Text [ en-US ] = "Background" ;
            };
        };
    };
};

#include "fmresids.hrc"
#include "svxids.hrc"
#include "globlmn.hrc"

ModalDialog RID_SVX_DLG_SHOWGRIDCOLUMNS
{
    OutputSize = TRUE;
    SVLook = TRUE ;
    Size = MAP_APPFONT( 200, 117 );
    Text [ de ] = "Spalten anzeigen";
    Moveable = TRUE;
    Closeable = TRUE;

    FixedText 1
    {
        Pos = MAP_APPFONT( 6, 6 );
        Size = MAP_APPFONT( 188, 30 );
        WordBreak = TRUE;
        Text [ de ] = "Folgende Spalten sind augenblicklich versteckt. Markieren Sie bitte die wieder einzublendenden und wählen Sie OK";
        Text [ en-US ] = "The following columns are currently hidden. Please mark the fields you want to show and choose OK.";
    };

    ListBox 1
    {
        Border = TRUE;
        Pos = MAP_APPFONT( 6, 39 );
        Size = MAP_APPFONT( 188, 55 );
        TabStop = TRUE;
        DropDown = FALSE;
        AutoHScroll = TRUE;
        VScroll = TRUE;
    };

    OKButton 1
    {
        Pos = MAP_APPFONT( 6, 97 );
        Size = MAP_APPFONT( 50, 14 );
        TabStop = TRUE;
        DefButton = TRUE ;
    };

    CancelButton 1
    {
        Pos = MAP_APPFONT( 144, 97 );
        Size = MAP_APPFONT( 50, 14 );
        TabStop = TRUE;
    };
    Text [ en-US ] = "Show columns";
    Text [ x-comment ] = " ";
};































    {
        MenuItem
        {
            Identifier = CM_NEXT_SLIDE ;
            Text [ en-US ] = "~Next" ;
        };
        MenuItem
        {
            Identifier = CM_PREV_SLIDE ;
            Text [ en-US ] = "~Previous" ;
           };
        MenuItem
        {
            Identifier = CM_GOTO;
            Text [ en-US ] = "~Go to Slide" ;

                        Text [ en-US ] = "~Last Slide";
                    };
                    MenuItem
                    {
                        Separator = TRUE;
                    };
                };
            };
        };
        MenuItem
        {
            Separator = TRUE;
        };
        MenuItem
        {
            Identifier = CM_SCREEN;
            Text [ en-US ] = "~Screen" ;

            SubMenu = Menu
            {
                ItemList =
                {
                    MenuItem
                    {
                        Identifier = CM_SCREEN_BLACK;
                        Text [ en-US ] = "~Black";
                    };
                    MenuItem
                    {
                        Identifier = CM_SCREEN_WHITE;
                        Text [ en-US ] = "~White";
                    };
                };
            };
        };
        MenuItem
        {
            Separator = TRUE;
        };
        MenuItem
        {
            Identifier = CM_ENDSHOW ;
            Text [ en-US ] = "~End Show" ;
        };
    };
};

            {
                Identifier = TABPAGE_CHARACTERS_EXT ;
                PageResID = TABPAGE_CHARACTERS_EXT ;
                Text [ de ] = "Schrifteffekt" ;
                Text [ en-US ] = "Font Effects" ;
            };
        };
    };
};

TabPage RID_PAGE_FIELDMAPPING
{
    SVLook  = TRUE ;
    Size    = MAP_APPFONT ( WINDOW_SIZE_X , WINDOW_SIZE_Y ) ;
    Text = "Feld-Zuordnung";
    Text [ english ] = "Field assigment";
    Text [ english_us ] = "Field assigment";


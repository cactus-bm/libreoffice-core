        Pos     = MAP_APPFONT ( 4, 59 ) ;
        Size    = MAP_APPFONT ( WINDOW_SIZE_X - 8, 8 ) ;

        Text = "Typ";
        Text [ english ] = "Type";
        Text [ english_us ] = "Type";
    };

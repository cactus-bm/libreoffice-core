    };
    PushButton PB_ASSIGN
    {
        Pos = MAP_APPFONT ( COL4 , ROW0  ) ;
        Size = MAP_APPFONT ( RSC_CD_PUSHBUTTON_WIDTH , RSC_CD_PUSHBUTTON_HEIGHT ) ;

{
    BUTTONS = WB_OK ;
    DEFBUTTON = WB_DEF_OK ;
    Message [ de ] = "%PRODUCTNAME konnte keine funktionierende E-Mail Konfiguration finden. Bitte speichern sie dieses Dokument lokal und senden es dann mit ihrem E-Mail Programm." ;

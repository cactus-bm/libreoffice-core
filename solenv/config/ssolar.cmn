wntmsci3
{
	order common
	environment
	{
		common
		{
			*build set mk_tmp=#@unique[#tmp#] ^ set mk_tmp=#@path[#mk_tmp#]#@name[#mk_tmp#].bat ^ #SOLARENV#\wnti\build -file #mk_tmp# #1& ^ call #mk_tmp# ^ del #@path[#mk_tmp#]#@name[#mk_tmp#].* >& NUL ^ unset mk_tmp
			SOLARSRC o:\%UPD%
		}
	}
}


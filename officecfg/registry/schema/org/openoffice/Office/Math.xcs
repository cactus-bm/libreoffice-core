          <label xml:lang="en-US">Textmode</label>
        </info>
      </prop>
      <prop oor:name="ScaleNormalBracket" oor:type="xs:boolean">
        <!-- OldPath: Math/StandardFormat -->
        <!-- OldLocation: Soffice.cfg -->

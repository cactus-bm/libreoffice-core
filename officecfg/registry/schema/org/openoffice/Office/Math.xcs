        <constraints>
          <enumeration oor:value="0">
            <info>
              <desc xml:lang="en-US">FAMILY_DONTKNOW</desc>
            </info>
          </enumeration>
          <enumeration oor:value="1">
            <info>
              <desc xml:lang="en-US">FAMILY_DECORATIVE</desc>
            </info>
          </enumeration>
          <enumeration oor:value="2">
            <info>
              <desc xml:lang="en-US">FAMILY_MODERN</desc>
            </info>
          </enumeration>
          <enumeration oor:value="3">
            <info>
              <desc xml:lang="en-US">FAMILY_ROMAN</desc>
            </info>
          </enumeration>
          <enumeration oor:value="4">
            <info>
              <desc xml:lang="en-US">FAMILY_SCRIPT</desc>
            </info>
          </enumeration>
          <enumeration oor:value="5">
            <info>
              <desc xml:lang="en-US">FAMILY_SWISS</desc>
            </info>
          </enumeration>
          <enumeration oor:value="6">
            <info>
              <desc xml:lang="en-US">FAMILY_SYSTEM</desc>
            </info>

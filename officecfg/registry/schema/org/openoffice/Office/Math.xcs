        <value>false</value>
        <info>
          <desc xml:lang="en-US"/>
        </info>
      </prop>
      <prop oor:name="FontFormatId" oor:type="xs:string"/>
    </group>
  </templates>
  <component>
    <group oor:name="Print">
      <prop oor:name="Title" oor:type="xs:boolean">
        <!-- OldPath: Math/Print -->
        <!-- OldLocation: Soffice.cfg -->
        <!-- Responsible: TL -->
        <!-- UIHints: Tools - Options  Formula  Settings - [Section] Print options -->
        <value>true</value>
        <info>
          <desc xml:lang="en-US">Printing option / print title (filename) of formula</desc>
          <label xml:lang="en-US">Title row</label>
        </info>
      </prop>
      <prop oor:name="FormulaText" oor:type="xs:boolean">
        <!-- OldPath: Math/Print -->
        <!-- OldLocation: Soffice.cfg -->
        <!-- Responsible: TL -->
        <!-- UIHints: Tools - Options  Formula  Settings - [Section] Print options -->
        <value>true</value>
        <info>
          <desc xml:lang="en-US">Printing option / print text of formula</desc>
          <label xml:lang="en-US">Formula text</label>
        </info>
      </prop>
      <prop oor:name="Frame" oor:type="xs:boolean">
        <!-- OldPath: Math/Print -->
        <!-- OldLocation: Soffice.cfg -->
        <!-- Responsible: TL -->
        <!-- UIHints: Tools - Options  Formula  Settings - [Section] Print options -->
        <value>true</value>
        <info>
          <desc xml:lang="en-US">Printing option / draw border around formula</desc>
          <label xml:lang="en-US">Border</label>
        </info>
      </prop>
      <prop oor:name="Size" oor:type="xs:short">
        <!-- OldPath: Math/Print -->
        <!-- OldLocation: Soffice.cfg -->
        <!-- Responsible: TL -->
        <!-- UIHints: Tools - Options  Formula  Settings - [Section] Print format -->
        <value>0</value>
        <info>
          <desc xml:lang="en-US">Printing option / size of a formula PRINT_SIZE_NORMAL = original size; PRINT_SIZE_SCALED = size will be adapted to paper size; PRINT_SIZE_ZOOMED = size will be defined by the zoom factor</desc>
          <label xml:lang="en-US">Print Size</label>
        </info>
        <constraints>
          <enumeration oor:value="0">
            <info>
              <desc xml:lang="en-US">PRINT_SIZE_NORMAL</desc>
            </info>
          </enumeration>
          <enumeration oor:value="1">
            <info>
              <desc xml:lang="en-US">PRINT_SIZE_SCALED</desc>
            </info>
          </enumeration>
          <enumeration oor:value="2">
            <info>
              <desc xml:lang="en-US">PRINT_SIZE_ZOOMED</desc>
            </info>
          </enumeration>
        </constraints>
      </prop>
      <prop oor:name="ZoomFactor" oor:type="xs:short">
        <!-- OldPath: Math/Print -->
        <!-- OldLocation: Soffice.cfg -->
        <!-- Responsible: TL -->
        <!-- UIHints: Tools - Options  Formula  Settings - [Section] Print format -->
        <value>100</value>
        <info>
          <desc xml:lang="en-US">Printing option / zoomfactor for printing of formulas. Only valid with PRINT_SIZE_ZOOMED.  [UNIT=%]</desc>
          <label xml:lang="en-US">Zoom factor</label>
        </info>
        <constraints>
          <minInclusive oor:value="10"/>
          <maxInclusive oor:value="400"/>
        </constraints>
      </prop>
    </group>
    <group oor:name="Misc">
      <prop oor:name="NoSymbolsWarning" oor:type="xs:boolean">
        <!-- OldPath: Math/Misc -->
        <!-- OldLocation: Soffice.cfg -->
        <!-- Responsible: TL -->
        <!-- UIHints: - -->
        <value>true</value>
        <info>
          <desc xml:lang="en-US">If TRUE a warning message will be shown when the symbol file used by the formulas is missing.</desc>
          <label xml:lang="en-US">Missing symbols warning</label>
        </info>
      </prop>
      <prop oor:name="IgnoreSpacesRight" oor:type="xs:boolean">
        <!-- OldPath: Math/Misc -->
        <!-- OldLocation: Soffice.cfg -->
        <!-- Responsible: TL -->
        <!-- UIHints: Tools - Options  Formula  Settings - [Section] Miscellaneous options -->
        <value>true</value>
        <info>
          <desc xml:lang="en-US">Option for ignoring spaces at the end of a line</desc>
          <label xml:lang="en-US">Ignore ~ and &amp;apos; at the end of the line</label>
        </info>
      </prop>
    </group>
    <group oor:name="View">
      <prop oor:name="ToolboxVisible" oor:type="xs:boolean">
        <!-- OldPath: Math/View -->
        <!-- OldLocation: Soffice.cfg -->
        <!-- Responsible: TL -->
        <!-- UIHints: View - Selection -->
        <value>true</value>
        <info>
          <desc xml:lang="en-US">Toggles the visibility of the ToolBox (Selection Box)</desc>
          <label xml:lang="en-US">Show Tool Box</label>
        </info>
      </prop>
      <prop oor:name="AutoRedraw" oor:type="xs:boolean">
        <!-- OldPath: Math/View -->
        <!-- OldLocation: Soffice.cfg -->
        <!-- Responsible: TL -->
        <!-- UIHints: View - AutoRefresh Display -->
        <value>true</value>
        <info>
          <desc xml:lang="en-US">If TRUE the display of the formula will be automatically updated after some time when the text has changed.</desc>
          <label xml:lang="en-US">Update display automatically</label>
        </info>
      </prop>
      <prop oor:name="FormulaCursor" oor:type="xs:boolean">
        <!-- OldPath: Math/View -->
        <!-- OldLocation: Soffice.cfg -->
        <!-- Responsible: TL -->
        <!-- UIHints: The icon &quot;Formula Cursor&quot; in the Tool bar -->
        <value>true</value>
        <info>
          <desc xml:lang="en-US">Toggles the visibility of the FormulaCursor</desc>
          <label xml:lang="en-US">Show Formula Cursor</label>
        </info>
      </prop>
    </group>
    <set oor:name="FontFormatList" oor:node-type="FontFormat"/>
    <group oor:name="StandardFormat">

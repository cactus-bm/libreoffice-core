            </info>
          </enumeration>
          <enumeration oor:value="1">
            <info>
              <desc xml:lang="en-US">PITCH_FIXED</desc>
            </info>
          </enumeration>
          <enumeration oor:value="2">
            <info>
              <desc xml:lang="en-US">PITCH_VARIABLE</desc>
            </info>
          </enumeration>
        </constraints>
      </prop>
      <prop oor:name="Weight" oor:type="xs:short">
        <constraints>
          <enumeration oor:value="0">
            <info>
              <desc xml:lang="en-US">WEIGHT_DONTKNOW</desc>
            </info>
          </enumeration>
          <enumeration oor:value="1">
            <info>
              <desc xml:lang="en-US">WEIGHT_THIN</desc>
            </info>
          </enumeration>
          <enumeration oor:value="2">
            <info>
              <desc xml:lang="en-US">WEIGHT_ULTRALIGHT</desc>
            </info>
          </enumeration>
          <enumeration oor:value="3">
            <info>
              <desc xml:lang="en-US">WEIGHT_LIGHT</desc>
            </info>
          </enumeration>
          <enumeration oor:value="4">
            <info>
              <desc xml:lang="en-US">WEIGHT_SEMILIGHT</desc>
            </info>
          </enumeration>
          <enumeration oor:value="5">
            <info>
              <desc xml:lang="en-US">WEIGHT_NORMAL</desc>
            </info>
          </enumeration>
          <enumeration oor:value="6">
            <info>
              <desc xml:lang="en-US">WEIGHT_MEDIUM</desc>
            </info>
          </enumeration>
          <enumeration oor:value="7">
            <info>
              <desc xml:lang="en-US">WEIGHT_SEMIBOLD</desc>
            </info>
          </enumeration>
          <enumeration oor:value="8">
            <info>
              <desc xml:lang="en-US">WEIGHT_BOLD</desc>
            </info>
          </enumeration>
          <enumeration oor:value="9">
            <info>
              <desc xml:lang="en-US">WEIGHT_ULTRABOLD</desc>
            </info>
          </enumeration>
          <enumeration oor:value="10">
            <info>
              <desc xml:lang="en-US">WEIGHT_BLACK</desc>
            </info>
          </enumeration>
        </constraints>
      </prop>
      <prop oor:name="Italic" oor:type="xs:short">
        <constraints>
          <enumeration oor:value="0">
            <info>
              <desc xml:lang="en-US">ITALIC_NONE</desc>
            </info>
          </enumeration>
          <enumeration oor:value="1">
            <info>
              <desc xml:lang="en-US">ITALIC_OBLIQUE</desc>
            </info>
          </enumeration>
          <enumeration oor:value="2">
            <info>
              <desc xml:lang="en-US">ITALIC_NORMAL</desc>
            </info>
          </enumeration>
          <enumeration oor:value="3">
            <info>
              <desc xml:lang="en-US">ITALIC_DONTKNOW</desc>
            </info>
          </enumeration>

          <enumeration oor:value="1">
            <info>
              <desc xml:lang="en-US">AlignCenter</desc>
            </info>
          </enumeration>
          <enumeration oor:value="2">
            <info>
              <desc xml:lang="en-US">AlignRight</desc>
            </info>
          </enumeration>
        </constraints>
      </prop>

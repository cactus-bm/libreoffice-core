#include "misc.hrc"
#include "helpid.h"
#include "cmdid.h"
#include "frmui.hrc"
TabDialog DLG_DOC_FOOTNOTE
{
    OutputSize = TRUE ;
    SVLook = TRUE ;
    Text [ en-US ] = "Footnotes/Endnotes Settings" ;
    Moveable = TRUE ;
    Sizeable = TRUE ;
    TabControl 1
    {
        OutputSize = TRUE ;
        PageList =
        {
            PageItem
            {
                Identifier = TP_FOOTNOTEOPTION ;
                PageResID = TP_FOOTNOTEOPTION ;
                Text [ en-US ] = "Footnotes" ;
            };
            PageItem
            {
                Identifier = TP_ENDNOTEOPTION ;
                PageResID = TP_ENDNOTEOPTION ;
                Text [ en-US ] = "Endnotes" ;
            };
        };
    };
};

#include "ucbexplorer.hrc"

Menu MENU_MAIN
{
    ItemList =

    {
        MenuItem
        {
            Identifier = MENU_FILE ;
            Text [ en-US ] = "~File" ;
            SubMenu = Menu
            {
                ItemList =

                {
                    MenuItem
                    {
                        Identifier = MENU_EXIT ;
                        Text [ en-US ] = "~Exit" ;
                        AccelKey = KeyCode
                        {
                            Function = KEYFUNC_QUIT ;
                        };
                    };
                };
            };
        };
    };
};

Menu MENU_POPUP
{
    ItemList =

    {

<oor:component-data oor:name="Jobs" oor:package="org.openoffice.Office" xmlns:oor="http://openoffice.org/2001/registry" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <node oor:name="Jobs">
    <node oor:name="com.sun.star.jobs.stclient" oor:op="fuse">
      <prop oor:name="Service">
        <value>com.sun.star.comp.framework.ShellJob</value>
      </prop>
      <node oor:name="Arguments">
        <prop oor:name="Command" oor:op="replace" oor:type="xs:string">
          <value>$(prog)/stclient_wrapper</value>
        </prop>
        <prop oor:name="Arguments" oor:op="replace" oor:type="xs:string">
          <value>-p "${SERVICETAG_PRODUCTNAME}" -e "${SERVICETAG_PRODUCTVERSION}" -t "${SERVICETAG_URN}" -S "${SERVICETAG_SOURCE}" -P "${SERVICETAG_PARENTNAME}"</value>
        </prop>
        <prop oor:name="NeedsSystemPathConversion" oor:op="replace" oor:type="xs:boolean">
          <value>true</value>
        </prop>
        <prop oor:name="DeactivateJobIfDone" oor:op="replace" oor:type="xs:boolean">
          <value>true</value>
        </prop>
      </node>
    </node>
  </node>
  <node oor:name="Events">
    <node oor:name="OnStartApp" oor:op="fuse">
      <node oor:name="JobList">
        <node oor:name="com.sun.star.jobs.stclient" oor:op="fuse" />
      </node>
    </node>
  </node>
</oor:component-data>
